<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/list_item_backroundcolor"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dimen_40dp"
    android:minHeight="@dimen/dimen_40dp"
    android:paddingLeft="@dimen/dimen_15dp"
    android:paddingRight="@dimen/dimen_15dp">

    <TextView
        android:id="@+id/txv_item_elv_log_datetime"
        style="@style/text_backup_log_name_color"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true" />

    <TextView
        android:id="@+id/txv_item_backup_log_num"
        style="@style/text_backup_log_num_color"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true" />
</RelativeLayout>
