<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/all_frg_view_color"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.maya.android.vcard.ui.frg.SettingPrivacySwapModelFragment">

   <LinearLayout
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       style="@style/all_btn_background_color"
       android:orientation="vertical">
       <View style="@style/vertical_line_divider_height_dimen_10dp"/>
       <RelativeLayout
           android:id="@+id/rel_privacy_swap_slip_vcard"
           android:layout_width="match_parent"
           android:layout_height="@dimen/dimen_50dp"
           android:paddingLeft="@dimen/dimen_15dp"
           android:paddingRight="@dimen/dimen_15dp">

           <TextView
               style="@style/text_setting_general_size_and_color"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_centerVertical="true"
               android:drawableLeft="@mipmap/img_privacy_swap_slip"
               android:drawablePadding="@dimen/dimen_5dp"
               android:gravity="center_vertical"
               android:text="@string/frg_setting_privacy_swap_slip_vcard" />

           <TextView
               android:id="@+id/txv_privacy_swap_slip_vcard"
               style="@style/text_setting_general_right_sign_size_and_color"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_alignParentRight="true"
               android:layout_centerVertical="true"
               android:drawablePadding="@dimen/dimen_10dp"
               android:drawableRight="@mipmap/img_general_right_arrow"
               android:text="" />
       </RelativeLayout>
       <View style="@style/horizontal_line_divider_padding_dimen_15dp"/>
       <RelativeLayout
           android:id="@+id/rel_privacy_swap_scan_vcard"
           android:layout_width="match_parent"
           android:layout_height="@dimen/dimen_50dp"
           android:paddingLeft="@dimen/dimen_15dp"
           android:paddingRight="@dimen/dimen_15dp">

           <TextView
               style="@style/text_setting_general_size_and_color"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_centerVertical="true"
               android:drawableLeft="@mipmap/img_privacy_swap_qr_code"
               android:drawablePadding="@dimen/dimen_5dp"
               android:text="@string/frg_setting_privacy_swap_scan_vcard" />

           <TextView
               android:id="@+id/txv_privacy_swap_scan_vcard"
               style="@style/text_setting_general_right_sign_size_and_color"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_alignParentRight="true"
               android:layout_centerVertical="true"
               android:drawablePadding="@dimen/dimen_10dp"
               android:drawableRight="@mipmap/img_general_right_arrow"
               android:text="" />
       </RelativeLayout>
       <View style="@style/horizontal_line_divider_padding_dimen_15dp"/>
       <RelativeLayout
           android:id="@+id/rel_privacy_swap_rock_vcard"
           android:layout_width="match_parent"
           android:layout_height="@dimen/dimen_50dp"
           android:paddingLeft="@dimen/dimen_15dp"
           android:paddingRight="@dimen/dimen_15dp">

           <TextView
               style="@style/text_setting_general_size_and_color"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_centerVertical="true"
               android:drawableLeft="@mipmap/img_privacy_swap_rock"
               android:drawablePadding="@dimen/dimen_5dp"
               android:text="@string/frg_setting_privacy_swap_rock_vcard" />

           <TextView
               android:id="@+id/txv_privacy_swap_rock_vcard"
               style="@style/text_setting_general_right_sign_size_and_color"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_alignParentRight="true"
               android:layout_centerVertical="true"
               android:drawablePadding="@dimen/dimen_10dp"
               android:drawableRight="@mipmap/img_general_right_arrow"
               android:text="" />
       </RelativeLayout>
   </LinearLayout>

</RelativeLayout>
